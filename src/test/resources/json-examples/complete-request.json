{
  "inputOntologyPath": "/path/to/input.owl",
  "outputOntologyPath": "/path/to/output.owl",
  "commands": [
    {
      "@type": "AnnotateCommand",
      "ontologyIri": "http://example.org/my-ontology",
      "versionIri": "http://example.org/my-ontology/v1.0.0",
      "annotations": [
        {
          "@type": "PlainAnnotation",
          "property": "rdfs:label",
          "value": "My Example Ontology"
        },
        {
          "@type": "LanguageAnnotation",
          "property": "dc:title",
          "value": "Mon Ontologie d'Exemple",
          "lang": "fr"
        },
        {
          "@type": "TypedAnnotation",
          "property": "dc:date",
          "value": "2025-11-04",
          "type": "xsd:date"
        },
        {
          "@type": "LinkAnnotation",
          "property": "dc:license",
          "value": "https://creativecommons.org/licenses/by/4.0/"
        }
      ],
      "flags": ["INTERPOLATE"]
    },
    {
      "@type": "ExtractCommand",
      "extractStrategy": {
        "@type": "SLME",
        "method": "BOT",
        "terms": [
          "GO:0008150",
          "GO:0003674",
          "GO:0005575"
        ]
      },
      "extractIntermediates": "minimal",
      "handlingImports": "include",
      "copyOntologyAnnotations": true
    },
    {
      "@type": "CollapseCommand",
      "threshold": 5,
      "preciousTerms": [
        "GO:0008150",
        "GO:0003674"
      ]
    },
    {
      "@type": "ExpandCommand",
      "expandTerms": [
        "UBERON:0000001"
      ],
      "noExpandTerms": [],
      "annotateExpansionAxioms": true
    },
    {
      "@type": "ConvertCommand",
      "format": "owl",
      "check": true,
      "cleanOboOptions": null,
      "addPrefixes": null
    }
  ]
}
